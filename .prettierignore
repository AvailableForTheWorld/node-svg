# Ignore artifacts:
node_modules
.DS_Store

# Ignore build output:
dist
build
.next
.nuxt
.svelte-kit

# Ignore lockfiles
package-lock.json
pnpm-lock.yaml
yarn.lock

# Ignore logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dotfiles
.env
.env.*
!.env.example

# IDE specific files
.idea
.vscode/*
!.vscode/settings.json
!.vscode/extensions.json
